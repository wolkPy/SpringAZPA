
public enum Code {
	
	// Database Errors
	INTERNAL_DATABASE_ERROR(Type.DATABASE_ERROR, "g1300"),
	
	// IO Errors
	INTERNAL_IO_ERROR(Type.IO_ERROR, "g1310"),
	
	// HTTP & API Errors
	
	INVALID_AUTH_VERSION(Type.API_ERROR, "a2110"),
	INVALID_CLIENT_TYPE_CLIENT_VERSION(Type.API_ERROR, "a2120"),
	
	INVALID_HTTP_COMMAND(Type.API_ERROR, "g1200"),
	INVALID_PARAMETERS(Type.API_ERROR, "g2020"),
	
	// Device Errors
	INVALID_STATUS_FOR_DEVICE(Type.APPLICATION_ERROR, "a3010"),
	INVALID_CLIENT_TYPE_FOR_DEVICE(Type.APPLICATION_ERROR, "a3020"),
	
	// Login Errors
	INVALID_SSIGN_ON_LOGIN(Type.SECURITY_ERROR, "a1530"),
	LOGIN_DOES_NOT_EXIST(Type.SECURITY_ERROR, "a1510"),
	LOGIN_INVALID_CREDENTIALS(Type.SECURITY_ERROR, "a1511"),
	INVALID_USER_STATUS(Type.SECURITY_ERROR, "a1512"),
	LOGIN_HAS_NO_ASSOCIATED_CLIENTS(Type.APPLICATION_ERROR, "a1513"),
	
	// Access Token Errors
	INVALID_ACCESS_TOKEN_FORMAT(Type.SECURITY_ERROR, "a1630"),
	
	// Session Errors
	SESSION_NOT_FOUND(Type.APPLICATION_ERROR, "s1730"),
	SESSION_EXPIRED(Type.APPLICATION_ERROR, "s1731"),
	INVALID_SESSION_STATUS(Type.APPLICATION_ERROR, "s1732"),
	SESSION_HAS_NO_INTERNAL_CUSTOMERS(Type.APPLICATION_ERROR, "s1733"),
	INVALID_SESSION_TYPE(Type.APPLICATION_ERROR, "s1734"),
	
	// Account Errors
	INVALID_ACCOUNT_FOR_CUSTOMER(Type.SECURITY_ERROR, "s1830"),

	
	UNEXPECTED_ERROR(Type.UNEXPECTED_ERROR, "g1000"), 
	
	//Register 
	EMAIL_ALREADY_EXISTS(Type.APPLICATION_ERROR, "r25000"),
	USER_NO_EXISTS(Type.APPLICATION_ERROR, "r25001"),
	VALIDATION_TIME_EXPIRED(Type.APPLICATION_ERROR, "r25002"),
	WRONG_VERIFICATION_TOKEN(Type.APPLICATION_ERROR, "r25003"),
	ACTIVE_USER(Type.APPLICATION_ERROR, "r25004"),
	USER_WAITING(Type.APPLICATION_ERROR, "r25005"), 
	
	//FormPago
	BAD_REQUEST(Type.APPLICATION_ERROR, "fpc1000"), 
	DATA_NOT_EXISTS(Type.APPLICATION_ERROR, "g30000");
	
	;
	
	private Type type;
	private String code;
	
	Code(Type type, String code){
		this.type = type;
		this.code = code;
	}

	public Type getType() {
		return type;
	}

	public String getCode() {
		return code;
	}

}
